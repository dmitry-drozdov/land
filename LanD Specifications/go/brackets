
CURVE_BRACKETED : %left '{' %right '}'
//ROUND_BRACKETED : %left '(' %right ')'
//SQUARE_BRACKETED : %left ('['|GENERAL_ATTRIBUTE_START) %right ']'


content	= entity*
entity	= curve | Any

curve = '{' (curve | Any)*  '}'


%%

%parsing {
	recovery
	fragment 
	start content
	skip 
}

%nodes {
	ghost entity 
	leaf 
	void
}



%markup {
	land 
	headercore(f_name) 
}
